nav.navbar.navbar-expand-lg.navbar-light.bg-dark.fixed-top
    a.navbar-brand(href='/') Home
    button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarColor02', aria-controls='navbarColor02', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
    #navbarColor02.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
            li.nav-item
                a.nav-link(href='/student/login') Student
            li.nav-item
                a.nav-link(href='/inst/login') Institution
            li.nav-item
                a.nav-link(href='/admin/login') Admin
        form.form-inline.my-2.my-lg-0
            if isAuthenticated
                if isAuthenticated === true
                    if session
                        if session.passport
                            if session.passport.user
                                if session.passport.user.type
                                    if session.passport.user.type === 'inst'
                                        .h4 Logged in as: Institution User
                                    if session.passport.user.type === 'student'
                                        .h4 Logged in as: Student User
                                    if session.passport.user.type === 'admin'
                                        .h4 Logged in as: Admin User
            else
                .h4 Not Logged In
